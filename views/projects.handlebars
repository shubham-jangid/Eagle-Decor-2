<!DOCTYPE html>
<html lang="en">

<head>
	<title>Eagle Decor -projects</title>
	<meta charset="utf-8">
	<meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no"> {{>meta}}{{>stylesheet}}
</head>

<body>

	{{>navigation}}
	<!-- END nav -->

	<section class="home-slider js-fullheight owl-carousel">
		<div class="slider-item js-fullheight" style="background-image:url(../public/images/bg_1.jpg);">
			<div class="overlay"></div>
			<div class="container">
				<div class="row slider-text justify-content-center align-items-center">

					<div class="col-md-7 col-sm-12 text-center ftco-animate">
						<h1 class="mb-3 mt-5 bread">Projects</h1>
						<p class="breadcrumbs">
							<span class="mr-2">
								<a href="/">Home</a>
							</span>
							<span>Projects</span>
						</p>
					</div>

				</div>
			</div>
		</div>
	</section>

	<section class="ftco-section">
		<div class="container-fluid">
			<div class="row no-gutters">



				<div class="col-md-6 col-lg-4 ftco-animate">
					<div class="project">
						<a href="projects/residence-villa-greater-kailash">
							<img src="../public/images/projects/residence-villa-greater-kailash/living-dining-and-bar-design-eagle-decor.jpg" class="img-fluid"
							 alt="eagle decor">
							<div class="text">
								<h3>Residence Villa| Greater Kailash</h3>
							</div>
						</a>

					</div>
				</div>
				<div class="col-md-6 col-lg-4 ftco-animate">
					<div class="project">
						<a href="projects/residence-vasant-kunj">

							<img src="../public/images/projects/residence-vasant-kunj/sofa-design-eagle-decor.jpg" class="img-fluid" alt="eagle decor">
							<div class="text">
								<h3>Residence | Vasant Kunj</h3>
							</div>
						</a>

					</div>
				</div>
				<div class="col-md-6 col-lg-4 ftco-animate">
					<a href="projects/duplex-residence-friends-colony">
						<div class="project">
							<img src="../public/images/projects/duplex-residence-friends-colony/arm-chair-eagle-decor.jpg" class="img-fluid" alt="eagle decor">
							<div class="text">
								<h3>Duplex Residence | Friends Colony</h3>
							</div>
					</a>
					</div>
				</div>
				<div class="col-md-6 col-lg-4 ftco-animate">
					<div class="project">
						<a href="projects/residence-friends-colony">

							<img src="../public/images/projects/Residence-friends-colony/console-design-eagle-decor.jpg" class="img-fluid" alt="eagle decor">
							<div class="text">
								<h3>Residence | Friends Colony</h3>
							</div>

						</a>

					</div>
				</div>
				<div class="col-md-6 col-lg-4 ftco-animate">
					<div class="project">
						<img src="../public/images/projects/duplex-residence-friends-colony/arm-chair-eagle-decor.jpg" class="img-fluid" alt="eagle decor">
						<div class="text">
							<h3>Duplex Residence | Friends Colony</h3>
						</div>

					</div>
				</div>



			</div>
	</section>

	{{>footer}}



	<!-- loader -->
	<div id="ftco-loader" class="show fullscreen">
		<svg class="circular" width="48px" height="48px">
			<circle class="path-bg" cx="24" cy="24" r="22" fill="none" stroke-width="4" stroke="#eeeeee" />
			<circle class="path" cx="24" cy="24" r="22" fill="none" stroke-width="4" stroke-miterlimit="10" stroke="#F96D00" />
		</svg>
	</div>

	{{>script}}

</body>

</html>